{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": [
        "feature",
        "feat"
      ],
      "rules": [
        {
          "on_property": "branch",
          "pattern": "^(feat|feature)/.+",
          "flags": "igu"
        },
        {
          "on_property": "title",
          "pattern": "^(feat|feature): .+",
          "flags": "igu"
        }
      ]
    },
    {
      "title": "## ğŸ› Fixes",
